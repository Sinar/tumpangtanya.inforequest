<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  xmlns:plone="http://namespaces.plone.org/plone"
  xmlns:zcml="http://namespaces.zope.org/zcml"
  i18n_domain="plone">

    <include package="plone.behavior" file="meta.zcml"/>

    <!-- -*- extra stuff goes here -*- -->

    <plone:behavior
        name="tumpangtanya.inforequest.submission_date"
        title="SubmissionDate"
        description="Date for information request submission"
        provides=".submission_date.ISubmissionDate"
        factory=".submission_date.SubmissionDate"
        marker=".submission_date.ISubmissionDateMarker"
        />


    <plone:behavior
        name="tumpangtanya.inforequest.government_agency"
        title="GovernmentAgency"
        description="Government Agency request as made to"
        provides=".government_agency.IGovernmentAgency"
        factory=".government_agency.GovernmentAgency"
        marker=".government_agency.IGovernmentAgencyMarker"
        />


</configure>
